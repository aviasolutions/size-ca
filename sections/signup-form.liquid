<div class="container hide">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h2 class="panel-title">Add your Address</h2>
    </div>
    <div class="panel-body">
      <input id="autocomplete" placeholder="Enter your address" onFocus="geolocate()" type="text" class="form-control">
      <div id="address">
        <div class="row">
          <div class="col-md-6">
            <label class="control-label">Street address</label>
            <input class="form-control" id="street_number" disabled="true">
          </div>
          <div class="col-md-6">
            <label class="control-label">Route</label>
            <input class="form-control" id="route" disabled="true">
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <label class="control-label">City</label>
            <input class="form-control field" id="locality" disabled="true">
          </div>
          <div class="col-md-6"> 
            <label class="control-label">State</label>
            <input class="form-control" id="administrative_area_level_1" disabled="true">
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <label class="control-label">Zip code</label>
            <input type="text" class="form-control" maxlength="10" id="postal_code">
          </div>
          <div class="col-md-6">
            <label class="control-label">Country</label>
            <input class="form-control" id="country" disabled="true">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


<div class="signup_form_confirmation" style="display:none">
  <div class="form_confirmation_wrap">
    <div class="form_confirmation_inner">
      {% if section.settings.confirm_heading != blank %}
      <h3>{{ section.settings.confirm_heading }}</h3>
      {% endif %}
      {% if section.settings.confirm_content != blank %}
      <div class="rte">{{ section.settings.confirm_content }}</div>
      {% endif %}
      {% if section.settings.confirm_btn_text != blank %}
      <a class="btn button" href="{{ section.settings.confirm_btn_link }}">{{ section.settings.confirm_btn_text }}</a>
      {% endif %}
    </div>
  </div>
</div>


                    {% for i in (1..31) %}
                    <span data-day="{{ i }}">{{ i }}</span>
                    {% endfor %}
                  </div>
                  <div data-tag="ctm_months" class="ctm_months" style="display:none;">
                    <span data-month="01">january</span>
                    <span data-month="02">february</span>
                    <span data-month="03">march</span>
                    <span data-month="04">april</span>
                    <span data-month="05">may</span>
                    <span data-month="06">jun</span>
                    <span data-month="07">july</span>
                    <span data-month="08">august</span>
                    <span data-month="09">september</span>
                    <span data-month="10">october</span>
                    <span data-month="11">november</span>
                    <span data-month="12">December</span>
                  </div>
                  <div data-tag="ctm_years" class="ctm_years" style="display:none;">
                    {% for i in (1960..2021) %}
                    <span>{{ i }}</span>
                    {% endfor %}
                  </div>
                </div>
              </div>
            </div>

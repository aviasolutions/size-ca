{% include 'mobile-menu' %}

{% if section.settings.announce_text != blank %}
<div class="announce_bar">
  <p>
    <sapn class="default_text ctm_location_based_span">{{ section.settings.announce_text }} </sapn>
    {% if section.settings.inside_ca_prod_less_text != blank %}
    <span class="inside_ca_prod_less ctm_location_based_span" style="display:none;">{{ section.settings.inside_ca_prod_less_text }} </span>
    {% endif %}
    {% if section.settings.inside_usa_prod_less_text != blank %}
    <span class="inside_usa_prod_less ctm_location_based_span" style="display:none;">{{ section.settings.inside_usa_prod_less_text }} </span>
    {% endif %}
    {% if section.settings.inside_ca_prod_over_text != blank %}
    <span class="inside_ca_prod_over ctm_location_based_span" style="display:none;">{{ section.settings.inside_ca_prod_over_text }} </span>
    {% endif %}
    {% if section.settings.inside_usa_prod_over_text != blank %}
     <span class="inside_usa_prod_over ctm_location_based_span" style="display:none;">{{ section.settings.inside_usa_prod_over_text }} </span>
    {% endif %}
    {% if section.settings.global_prod_text != blank %}
     <span class="global_msg ctm_location_based_span" style="display:none;">{{ section.settings.global_prod_text }} </span>
    {% endif %}
    {% if section.settings.announce_linkable_text != blank %} 
    <a href="{{ section.settings.announce_link }}">
      {{ section.settings.announce_linkable_text }}
    </a>
    {% endif %}
  </p>
</div>
{% endif %}

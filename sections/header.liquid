{% if template contains 'product' %}
<script>
  var ca_amount = parseInt("{{ section.settings.ca_amount }}") * 100;
  var usa_amount = parseInt("{{ section.settings.ca_amount }}") * 100;
  var prod_amount = parseInt("{{ product.selected_or_first_available_variant.price }}");                            
  jQuery.get("http://ipinfo.io", function(response) {
    console.log(response,ca_amount,prod_amount);
    var count = 0;
    if(response.country == 'CA'){
      console.log(prod_amount,ca_amount)
      if(ca_amount > prod_amount){
        count++;
//       	$('.inside_ca_prod_less').show();v
        var text = $('.inside_ca_prod_less').text();
        $('.product-single-shipping a span:not(.icon)').html('');
        $('.product-single-shipping a span:not(.icon)').html(text+'<br> Learn more');
      }else{
        count++;
//         $('.inside_ca_prod_over').show();	
        var text = $('.inside_ca_prod_over').text();
        $('.product-single-shipping a span:not(.icon)').html('');
        $('.product-single-shipping a span:not(.icon)').html(text+'<br> Learn more');
        console.log(text,'above')
      }
    }
    else if(response.country == 'US' || response.country == 'USA'){
      if(usa_amount > prod_amount){
        count++;
//         $('.inside_usa_prod_less').show();
        var text = $('.inside_usa_prod_less').text();
        $('.product-single-shipping a span:not(.icon)').html('');
        $('.product-single-shipping a span:not(.icon)').html(text+'<br> Learn more');
      }else{
        count++;
//         $('.inside_usa_prod_over').show();
        var text = $('.inside_usa_prod_over').text();
        $('.product-single-shipping a span:not(.icon)').html('');
        $('.product-single-shipping a span:not(.icon)').html(text+'<br> Learn more');
      }
    }else{
      count++;
//       $('.global_msg').show();
      var text = $('.global_msg').text();
      $('.product-single-shipping a span:not(.icon)').html('');
      $('.product-single-shipping a span:not(.icon)').html(text+'<br> Learn more');
    }
//     if(count == 0){
//      $('.default_text').show();
//     }
  }, "jsonp");
</script>
{% endif %}



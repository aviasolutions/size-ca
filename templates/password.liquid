    }
    /* width */
    .privacy__modal::-webkit-scrollbar {
      width: 5px;
    }

    /* Track */
    .privacy__modal::-webkit-scrollbar-track {
      background: #f1f1f1; 
    }

    /* Handle */
    .privacy__modal::-webkit-scrollbar-thumb {
      background: #888; 
    }

    /* Handle on hover */
    .privacy__modal::-webkit-scrollbar-thumb:hover {
      background: #555; 

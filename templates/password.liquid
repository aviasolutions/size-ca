    }
    
    
    .floating-label {
      position: relative;
      display:inline-block;      
    }
    .floating-input{
      font-size: 14px;
      padding: 4px 4px;
      display: block;
      width: 100%;
      height: 30px;
      background-color: transparent;
      border: none;
      border-bottom: 1px solid #757575;
    }

    .floating-input:focus{
      outline: none;
      border-bottom: 2px solid #5264ae;
    }

    .floating-label label {
      color: #AAAAAA;
      font-size: 18px !important;
      font-weight: normal !important;
      position: absolute;
      pointer-events: none;
      padding-bottom: 0 !important;
      left: 15px;
      top: 16px;
      transition: 0.2s ease all;
      -moz-transition: 0.2s ease all;
      -webkit-transition: 0.2s ease all;
    }

    .floating-input:focus ~ label,
    .floating-input:not(:placeholder-shown) ~ label {
      top: 6px;
      font-size: 13px !important;
    }
    .form-vertical .input-group .input-group__field-email:focus {
      padding-top: 10px !important;
    }
    .custom-signup_form::-webkit-scrollbar {
      width: 5px;
    }
    /* Track */
    .custom-signup_form::-webkit-scrollbar-track {
      background: #888; 
    }
    /* Handle */
    .custom-signup_form::-webkit-scrollbar-thumb {
      background: #f1f1f1; 
    }
    /* Handle on hover */
    .custom-signup_form::-webkit-scrollbar-thumb:hover {
      background: #fff; 
    }
    .desktop_hide{display:none;}
    
    .learn-more{display:none;}
    
    .actions-submit{position:relative;}    
    .actions-submit img{display:none;}
   .form-active .actions-submit img {
    position: absolute;
    top: 14px;
    left: 0;
    right: 0;
    margin: 0 auto; display:block;
}
    
     .form-active .actions-submit input{font-size:0px !important;}
   .form-active .term_check {    display: none !important;}
    .success-msg, .error-msg{
      color : #ffffff;
    }
    .success-msg a, .error-msg a{
      color : #ffffff;
      font-weight: 600;
    }   
    
    
    @media(max-width:768px){
      .custom-signup_form{display:block;}
      .ctm__form_inner{display:block; padding:20px;}
      .mobile_hide{display:none;}
      .desktop_hide{display:block;}
      .form-vertical .input-group{flex-direction:column;}
      .form-vertical .input-group .input-group__field-email{width:100% !important; max-width:320px;}
      .form-vertical #mc_embed_signup .button{width:100%;max-width:320px; margin:5px auto 0;}
      .floating-label{width:100% !important; max-width:320px; margin:0 auto;}
      .form_header .rte{font-size:20px; line-height:27px;}
      .footer__social .icon { width: 34px; height: 34px;}
      .password-powered-by small{font-size:14px; line-height:20px;}
      .password-content > img { max-width: 177px;}
      .privacy__modal{padding:20px;}
      
      
    }
    
  

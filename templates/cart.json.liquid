{% layout none %}

{
    "cart": {{- cart | json -}},
    "items":[
        {%- for item in cart.items -%}
            {
                "id": {{- item.id | json -}},
                "inventory_quantity": {{- item.variant.inventory_quantity | json -}}
            }{%- unless forloop.last -%},{%- endunless -%}
        {%- endfor -%}
    ]
}
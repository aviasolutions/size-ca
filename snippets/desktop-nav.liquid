					{% if block.settings.column1_image %}
                    <a class="column__img" href="{{ block.settings.column1_image_link }}">
                      <img src="{{ block.settings.column1_image | img_url:'master' }}">
                    </a>
                    {% else %}
                      {% if block.settings.column1_menu1 %}
                      <div class="menu_column">	
